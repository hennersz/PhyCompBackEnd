[  {    "type": "get",    "url": "/user/:id",    "title": "Request User information",    "name": "GetUser",    "group": "User",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "id",            "description": "<p>Users unique ID.</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "firstname",            "description": "<p>Firstname of the User.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "lastname",            "description": "<p>Lastname of the User.</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  \"firstname\": \"John\",\n  \"lastname\": \"Doe\"\n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "UserNotFound",            "description": "<p>The id of the User was not found.</p>"          }        ]      },      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 404 Not Found\n{\n  \"error\": \"UserNotFound\"\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "routes/tests.js",    "groupTitle": "User"  },  {    "type": "get",    "url": "/api/all",    "title": "Request all pollution data",    "name": "GetAllData",    "group": "pollution",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "latitiude",            "description": "<p>The latitude of the point you want to get data near to</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "longitude",            "description": "<p>The longitude of the point you want to get data near to</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "maxDist",            "description": "<p>The maximum distance from the specified point you want data from</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "_id",            "description": "<p>Mongo ID of the device</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "deviceID",            "description": "<p>Smart citizen devce ID</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "location",            "description": "<p>London air location</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "siteName",            "description": "<p>OpenAQ site name</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "datetime",            "description": "<p>The time the sensor was updated</p>"          },          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "data",            "description": "<p>An array of data objects</p>"